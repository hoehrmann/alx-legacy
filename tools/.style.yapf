[style]
based_on_style = pep8
indent_width = 2
column_limit = 70
dedent_closing_brackets = 1
continuation_indent_width = 2
blank_line_before_nested_class_or_def = 1
blank_line_before_module_docstring = 1
blank_line_before_class_docstring = 1
split_all_comma_separated_values = 1
split_before_dot = 1
